local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Phoenix | By Lost",
   Icon = 0,
   LoadingTitle = "Phoenix Hub",
   LoadingSubtitle = "by Lost",
   Theme = "Amethyst",

   DisableRayfieldPrompts = True,
   DisableBuildWarnings = False,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Big Hub"
   },
})

local Tab = Window:CreateTab("Blade Ball", 4483362458)

local Button = Tab:CreateButton({
   Name = "Visual Blade ball",
   Callback = function()
        loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/1432c65d5efeb637abfdf3863013744c.lua"))()
   end,
})

